{
  "functions": {
    "source": ".",
    "predeploy": "npm run build"
  },
  "hosting": {
    "public": "dist/client",
    "ignore": [
      "index.html",
      "ssr-manifest.json "
    ],
    "rewrites": [
      {
        "source": "**",
        "function": "ssr"
      }
    ]
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  }
}
